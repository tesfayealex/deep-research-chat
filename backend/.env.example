# Rename this file to .env and fill in your API keys

# OpenAI API Key (Required for LLM operations)
# Get yours from https://platform.openai.com/api-keys
OPENAI_API_KEY="YOUR_OPENAI_API_KEY"

# Tavily Search API Key (Required for the search tool)
# Get yours from https://app.tavily.com/
TAVILY_API_KEY="YOUR_TAVILY_API_KEY"

# Optional: Specify OpenAI model name
# OPENAI_MODEL_NAME="gpt-4o-mini"
OPENAI_MODEL_NAME="gpt-4o"

# Optional: LangSmith Tracing (for debugging LangChain/LangGraph)
# Get keys from https://smith.langchain.com/
# LANGCHAIN_TRACING_V2="true"
# LANGCHAIN_API_KEY="YOUR_LANGSMITH_API_KEY"
# LANGCHAIN_PROJECT="Deep Research Agent" # Optional: project name 